/**
 * @file ecenter_editing.install
 */

/**
 * Implementation of hook_install().
 *
 * Simply set thumbnail size.
 */
function ecenter_editing_install() {
  variable_set('imagefield_thumb_size', '60x60');
}

function ecenter_editing_uninstall() {
  variable_del('imagefield_thumb_size');
}
