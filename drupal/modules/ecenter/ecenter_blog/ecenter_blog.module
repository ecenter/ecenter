include_once('ecenter_blog.features.inc');

/**
 * Implementation of hook_form_alter().
 */
function ecenter_blog_form_alter(&$form, $form_state, $form_id) {
  if ($form_id == 'blog_post_node_form') {
    $form['field_blog_post_authors']['field_blog_post_authors_add_more']['#value'] = t('Add another author');
  }
}
